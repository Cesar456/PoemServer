<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cesar.poem.bean.PoemTagDetail" table="poem_tag_detail" catalog="poemcesar" dynamic-insert="true" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="poemID" type="java.lang.Integer">
            <column name="poemID"  />
        </property>
        <property name="tag" type="java.lang.String">
            <column name="tag" />
        </property>
    </class>
</hibernate-mapping>
